<!DOCTYPE flight_plan SYSTEM "../flight_plan.dtd">

<flight_plan alt="110" ground_alt="10" lat0="69.295143" lon0="16.0291" max_dist_from_home="1000" name="Andenes ASC" qfu="0" security_height="400">
  <header>
#include "subsystems/navigation/nav_line.h"
#include "subsystems/datalink/datalink.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="-69.1" y="-56.8"/>
    <waypoint name="1" x="-340.3" y="46.0"/>
    <waypoint name="2" x="-246.5" y="-58.8"/>
    <waypoint name="S1" x="-466.0" y="275.7"/>
    <waypoint name="S2" x="-398.7" y="257.9"/>
    <waypoint name="MOB" x="-264.8" y="83.6"/>
    <waypoint name="CR" x="-204.9" y="15.3"/>
    <waypoint alt="100.0" name="XC" x="-451.6" y="37.8"/>
    <waypoint name="START" x="-360.4" y="-23.7"/>
    <waypoint name="_BASELEG" x="-175.9" y="-173.9"/>
    <waypoint name="AF" x="-73.4" y="-94.5"/>
    <waypoint name="TD" x="-114.8" y="68.0"/>
    <waypoint name="P1" x="-468.4" y="166.4"/>
    <waypoint name="P2" x="-465.4" y="143.5"/>
    <waypoint name="P3" x="-481.9" y="148.4"/>
    <waypoint name="P4" x="-449.3" y="141.1"/>
  </waypoints>
  <exceptions>
    <exception cond="estimator_z > 750" deroute="wait"/>
    <exception cond="datalink_time > 140" deroute="wait"/>
    <exception cond="10.0 > PowerVoltage()" deroute="wait"/>
  </exceptions>
  <blocks>
    <block name="start">
      <attitude pitch="20" roll="0" throttle="0.99" until="(estimator_flight_time > 3)" vmode="throttle"/>
      <go wp="START"/>
    </block>
    <block name="wait" strip_button="wait">
      <circle radius="nav_radius" wp="CR"/>
    </block>
    <block name="circle" strip_button="wait">
      <circle radius="nav_radius" wp="XC"/>
    </block>
    <block name="prof1" strip_button="prof1">
      <set value="60" var="waypoints[WP_P1].a"/>
      <circle radius="nav_radius" until="NavCircleCount() > 3" wp="P1"/>
      <set value="650" var="waypoints[WP_P1].a"/>
      <circle radius="nav_radius" until="NavCircleCount() > 3" wp="P1"/>
      <set value="60" var="waypoints[WP_P1].a"/>
      <circle radius="nav_radius" until="NavCircleCount() > 3" wp="P1"/>
    </block>
    <block name="prof2" strip_button="prof2">
      <set value="60" var="waypoints[WP_P2].a"/>
      <circle radius="nav_radius" until="NavCircleCount() > 3" wp="P2"/>
      <set value="650" var="waypoints[WP_P2].a"/>
      <circle radius="nav_radius" until="NavCircleCount() > 3" wp="P2"/>
      <set value="60" var="waypoints[WP_P2].a"/>
      <circle radius="nav_radius" until="NavCircleCount() > 3" wp="P2"/>
    </block>
    <block name="go_prof3" strip_button="prof3">
      <set value="100+ground_alt" var="waypoints[WP_P3].a"/>
      <circle radius="nav_radius" until="NavCircleCount() > 1" wp="P3"/>
      <deroute block="prof3"/>
    </block>
    <block name="prof3">
      <set value="40+ground_alt" var="waypoints[WP_P3].a"/>
      <circle radius="nav_radius" until="stage_time > 1" wp="P3"/>
      <circle climb="-2" radius="nav_radius" until="5 > fabs(GetPosAlt() - WaypointAlt(WP_P3))" vmode="climb" wp="P3"/>
      <circle radius="nav_radius" until="NavCircleCount() > 1" wp="P3"/>
      <set value="650+ground_alt" var="waypoints[WP_P3].a"/>
      <circle radius="nav_radius" until="stage_time > 1" wp="P3"/>
      <circle climb="2" radius="nav_radius" until="5 > fabs(GetPosAlt() - WaypointAlt(WP_P3))" vmode="climb" wp="P3"/>
      <circle radius="nav_radius" until="NavCircleCount() > 1" wp="P3"/>
      <deroute block="prof3"/>
    </block>
    <block name="go_prof4" strip_button="prof4">
      <circle radius="nav_radius" until="NavCircleCount() > 1" wp="P4"/>
      <deroute block="prof4"/>
    </block>
    <block name="prof4">
      <set value="40+ground_alt" var="waypoints[WP_P4].a"/>
      <circle radius="nav_radius" until="5 > fabs(GetPosAlt() - WaypointAlt(WP_P4))" wp="P4"/>
      <circle radius="nav_radius" until="NavCircleCount() > 1" wp="P4"/>
      <set value="650+ground_alt" var="waypoints[WP_P4].a"/>
      <circle radius="nav_radius" until="5 > fabs(GetPosAlt() - WaypointAlt(WP_P4))" wp="P4"/>
      <circle radius="nav_radius" until="NavCircleCount() > 1" wp="P4"/>
      <deroute block="prof4"/>
    </block>
    <block name="down" strip_button="Down" strip_icon="downdown.png">
      <set value="LATERAL_MODE_ROLL" var="lateral_mode"/>
      <set value="V_CTL_MODE_MANUAL" var="v_ctl_mode"/>
      <set value="TRUE" var="h_ctl_disabled"/>
      <set value="(-0.6*MAX_PPRZ)" var="h_ctl_aileron_setpoint"/>
      <set value="(0.9*MAX_PPRZ)" var="h_ctl_elevator_setpoint"/>
      <while cond="estimator_z > ground_alt + 220"/>
      <set value="0" var="h_ctl_aileron_setpoint"/>
      <set value="0" var="h_ctl_elevator_setpoint"/>
    </block>
    <block name="down_end" strip_button="DownEnd" strip_icon="downdownend.png">
      <set value="0" var="h_ctl_aileron_setpoint"/>
      <set value="0" var="h_ctl_elevator_setpoint"/>
      <while cond="1.> stage_time"/>
      <set value="FALSE" var="h_ctl_disabled"/>
      <deroute block="wait"/>
    </block>
    <block name="Line 1-2" strip_button="Line (wp 1-2)" strip_icon="line.png">
      <call fun="nav_line_init()"/>
      <call fun="nav_line(WP_1, WP_2, nav_radius)"/>
    </block>
    <block name="Oval 1-2" strip_button="Oval (wp 1-2)" strip_icon="oval.png">
      <oval p1="1" p2="2" radius="nav_radius"/>
    </block>
    <block name="Survey S1-S2 NS" strip_button="Survey (wp S1-S2) NS" strip_icon="survey.png">
      <survey_rectangle grid="90" wp1="S1" wp2="S2"/>
    </block>
    <block name="Survey S1-S2 WE" strip_button="Survey (wp S1-S2) WE" strip_icon="survey_we.png">
      <survey_rectangle grid="90" orientation="WE" wp1="S1" wp2="S2"/>
    </block>
    <block name="MOB" strip_button="Circle here" strip_icon="mob.png">
      <call fun="NavSetWaypointHere(WP_MOB)"/>
      <set value="DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <circle radius="nav_radius" wp="MOB"/>
    </block>
    <block name="profile 1 up" strip_button="Profile 1 Up" strip_icon="up_profile.png">
      <for from="0" to="4" var="i">
        <set value="ground_alt+40+15*$i" var="waypoints[WP_P1].a"/>
        <circle radius="nav_radius" until="NavCircleCount() > 3" wp="P1"/>
      </for>
      <deroute block="profile 1 down"/>
    </block>
    <block name="profile 1 down" strip_button="Profile 1 Down" strip_icon="down_profile.png">
      <for from="0" to="4" var="i">
        <set value="ground_alt+100-15*$i" var="waypoints[WP_P1].a"/>
        <circle radius="nav_radius" until="NavCircleCount() > 3" wp="P1"/>
      </for>
      <deroute block="profile 1 up"/>
    </block>
    <block name="Land Right AF-TD" strip_button="Land right (wp AF-TD)" strip_icon="land-right.png">
      <set value="DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <deroute block="land"/>
    </block>
    <block name="Land Left AF-TD" strip_button="Land left (wp AF-TD)" strip_icon="land-left.png">
      <set value="-DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <deroute block="land"/>
    </block>
    <block name="land">
      <call fun="nav_compute_baseleg(WP_AF, WP_TD, WP__BASELEG, nav_radius)"/>
      <circle radius="nav_radius" until="NavCircleCount() > 0.5" wp="_BASELEG"/>
      <circle radius="nav_radius" until="NavQdrCloseTo(DegOfRad(baseleg_out_qdr)-(nav_radius/fabs(nav_radius))*10) && 20 > fabs(estimator_z - WaypointAlt(WP__BASELEG))" wp="_BASELEG"/>
    </block>
    <block name="final">
      <exception cond="ground_alt + 10 > estimator_z" deroute="flare"/>
      <go from="AF" hmode="route" vmode="glide" wp="TD"/>
    </block>
    <block name="flare">
      <go approaching_time="0" from="AF" hmode="route" throttle="0.0" vmode="throttle" wp="TD"/>
      <attitude roll="0.0" throttle="0.0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
